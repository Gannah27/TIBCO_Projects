<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/Files/Resources/Schemas/Services/ListFileRequest.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/ParseandRender/Resources/Schemas/ListFileResponse.xsd" xmlns:ns="http://www.tibco.com/schemas/ParseandRender/Resources/Schemas/Commons/Header.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/schemas/ParseandRender/Resources/Schemas/Commons/Response.xsd" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file">
    <xsd:import namespace="http://www.tibco.com/schemas/Files/Resources/Schemas/Services/ListFileRequest.xsd" schemaLocation="/Resources/Schemas/Services/ListFileRequest.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/ParseandRender/Resources/Schemas/ListFileResponse.xsd" schemaLocation="/Resources/Schemas/Services/ListFileResponse.xsd"/>
    <pd:name>Processes/Services/ListandreadFile.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:ListFileRequest"/>
    <pd:startX>86</pd:startX>
    <pd:startY>75</pd:startY>
    <pd:returnBindings>
        <pfx2:ListFileResponse>
            <xsl:copy-of select="$Start/pfx:ListFileRequest/ns:Header"/>
            <pfx2:Body>
                <ns2:Response>
                    <ns2:message>
                        <xsl:value-of select="&quot;Passed&quot;"/>
                    </ns2:message>
                    <ns2:ResponseStatus>
                        <xsl:value-of select="&quot;SUCCESS&quot;"/>
                    </ns2:ResponseStatus>
                </ns2:Response>
            </pfx2:Body>
        </pfx2:ListFileResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:ListFileResponse"/>
    <pd:endX>838</pd:endX>
    <pd:endY>83</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1738953788811</pd:targetNamespace>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>418</pd:x>
        <pd:y>80</pd:y>
        <config>
            <element>
                <xsd:element name="Files">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="fileinfo" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="fullName" type="xsd:string"/>
                                        <xsd:element name="filename" type="xsd:string"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <Files>
                <xsl:for-each select="$List-Files/ns1:ListFilesActivityOutput/files/fileInfo[contains(fileName,'.txt') and starts-with(fileName,'file_')]">
                    <fileinfo>
                        <fullName>
                            <xsl:value-of select="fullName"/>
                        </fullName>
                        <filename>
                            <xsl:value-of select="fileName"/>
                        </filename>
                    </fileinfo>
                </xsl:for-each>
            </Files>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="List Files">
        <pd:type>com.tibco.plugin.file.ListFilesActivity</pd:type>
        <pd:resourceType>ae.activities.ListFilesActivity</pd:resourceType>
        <pd:x>277</pd:x>
        <pd:y>71</pd:y>
        <config>
            <mode>only-files</mode>
        </config>
        <pd:inputBindings>
            <ns1:ListFilesActivityConfig>
                <fileName>
                    <xsl:value-of select="concat($Start/pfx:ListFileRequest/pfx:Body/pfx:sourcePath, $Start/pfx:ListFileRequest/pfx:Body/pfx:pattern)"/>
                </fileName>
            </ns1:ListFilesActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>516</pd:x>
        <pd:y>5</pd:y>
        <pd:width>199</pd:width>
        <pd:height>140</pd:height>
        <pd:collapsedWidth>52</pd:collapsedWidth>
        <pd:collapsedHeight>69</pd:collapsedHeight>
        <pd:groupVariables/>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Map-Data/Files/fileinfo</pd:over>
            <pd:iterationElementSlot>cFile</pd:iterationElementSlot>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Read File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Read File">
            <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
            <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
            <pd:x>602</pd:x>
            <pd:y>79</pd:y>
            <config>
                <encoding>text</encoding>
            </config>
            <pd:inputBindings>
                <ns0:ReadActivityInputClass xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file/v2">
                    <fileName>
                        <xsl:value-of select="$cFile/fileinfo/filename"/>
                    </fileName>
                </ns0:ReadActivityInputClass>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>List Files</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>List Files</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>